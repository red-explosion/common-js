{
  "name": "@red-explosion/eslint-config-ts",
  "version": "0.1.0",
  "description": "Red Explosion's TypeScript ESLint config rules",
  "keywords": [
    "eslint-config",
    "eslint",
    "red-explosion",
    "typescript"
  ],
  "homepage": "https://github.com/red-explosion/common-js/tree/main/packages/eslint-config-ts",
  "license": "MIT",
  "author": {
    "name": "Ben Sherred",
    "email": "ben@sherred.co.uk"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts",
    "test": "tsc --noEmit"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
